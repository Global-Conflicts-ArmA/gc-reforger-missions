  "2 Medics"\
  ""\
  "22 - 44 Troops"\
  ""\
  "1 Pilot every 11 Troops"\
  "1 Crew every Pilot"\
  ""\
  "Helicopter Restriction: 1 Heavy CAS or 2 Little Birds with Guns & Rockets"\
  ""\
  "   ----- Hard (59 - 85 Players) -----"\
  "1 CHQ & FTO"\
  "2 PHQ"\
  "4 Medics"\
  ""\
  "44 - 66 Troops"\
  ""\
  "4 Pilots"\
  "4 Pilot Crews"\
  ""\
  "Helicopter Restriction: 2 Heavy CAS OR 4 Little Birds with Guns & Rockets OR 1 Heavy CAS and 2 Little Birds"
  m_bEmptyFactionVisibility 1
  m_bShowForAnyFaction 1
  m_iOrder 1
 }
