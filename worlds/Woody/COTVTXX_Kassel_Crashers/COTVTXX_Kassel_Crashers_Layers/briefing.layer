  coords 381.797 160.4 891.225
  m_sTitle "Briefing: Notes."
  m_sTextData "VI. End Conditions:"\
  "Time Limit - Unlimited"\
  ""\
  "USSR Win:"\
  "All Objective Seized."\
  ""\
  "US Win:"\
  "80% USSR Casualties."
