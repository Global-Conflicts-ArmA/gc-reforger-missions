$grp PS_MissionDescription : "{3136BE42592F3B1B}PrefabsEditable/MissionDescription/EditableMissionDescription.et" {
 OPFOR1 {
  coords 6299.185 64.995 4669.115
  m_sTitle "CURRENT SITUATION"
  m_sTextData "We are 321 a Motorized BTR PLT with an attached BRDM PLT located south of Simon's Wood. "\
  ""\
  "Early this morning US Marines conducted a raid onto our backline depots and in turn our sister COY has been delayed from its task of reinforcing us. We are currently located at Simon's Wood's and digging in. Our Engineers have setup a minefield to force the enemy into our Enagement Area. Further, 7 bunkers and HMG positions have been setup (Makred on Map)."\
  ""
  m_aVisibleForFactions {
   "USSR"
  }
  m_iOrder 2
 }
 OPFOR2 {
  coords 6361.396 84.777 4686.647
  m_sTitle "ENEMY"
  m_sTextData "Enemy Forces are a COY of Light Armouned Reconnisance with an estimated 10 LAV25s and about a platoon size of dismounts or infantry."\
  "	"\
  "Confirmed enemy locations:"\
  "St. Montignac"\
  ""
  m_aVisibleForFactions {
   "USSR"
  }
  m_iOrder 3
 }
 OPFOR3 {
  coords 6145.719 48.648 4527.303
  m_sTitle "COMPANY ORDER"
  m_sTextData "I order:"\
  "	"\
  "321 PLT and 361 PLT to conduct a hasty defence against enemy recon elements until friendly reinforcements will arive (70 mins after mission start)."\
  "	"\
  "b) Tasks to artillery and other units:"\
  "	"\
  "320 Company mortars currently resupplying, and will not be available. "\
  ""
  m_aVisibleForFactions {
   "USSR"
  }
  m_iOrder 4
 }
 OPFOR5 {
  coords 6518.397 18.201 4420.083
  m_sTitle "METREP"
